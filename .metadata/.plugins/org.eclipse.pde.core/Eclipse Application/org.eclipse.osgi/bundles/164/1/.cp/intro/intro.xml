<?xml version="1.0" encoding="utf-8" ?>
<introContent>
  	<extensionContent id="comamazonawseclipseec2-introExtension"
  	                  style="css/awsToolkit.css"
  	                  name="AWS Toolkit Overview Extension"
  	                  path="overview/@">
		<group style-id="content-group" id="comamazonawseclipseec2-introLink-group">
       		<link label="AWS Toolkit for Eclipse"
       		      url="http://org.eclipse.ui.intro/showPage?id=awsToolkitOverview" 
       		      id="comamazonawseclipseec2-introLink"
       		      style-id="content-link">
          		<text>
          			Get familiar with the AWS Toolkit for Eclipse and all the features that
          			make building applications that leverage Amazon Web Services easy.
          		</text>
       		</link>
       	</group>
  	</extensionContent>

	      
    <page id="awsToolkitOverview"
          style="$theme$/html/overview.css, css/awsToolkit.css"
          style-id="page">
          
         <title style-id="intro-header">$introTitle$</title>
         <group id="extra-group1" filteredFrom="swt"/>
         <include path="overview/navigation-links"/>
         
         <group id="page-content">
			<html style-id="content-group"
			      type="inline" 
			      id="awsToolkitOverview-html"
			      src="awsToolkitOverview.xhtml"/>            
         </group>
    </page>

</introContent>
